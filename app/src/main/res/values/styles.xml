<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_material_dark</item>
        <item name="colorPrimaryDark">@color/primary_dark_material_dark</item>
        <item name="colorAccent">@color/Black</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorHint">@color/White</item>
        <item name="android:colorForeground">@color/AliceBlue</item>
        <item name="android:colorBackground">@color/Black</item>
    </style>


    <style name="ssn1" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorAccent">@android:color/black</item>
        <item name="android:colorForeground">@color/foreground_material_dark</item>

    </style>

    <style name="student" parent="AppTheme" />

    <style name="warden" parent="student">
        <item name="colorAccent">@android:color/darker_gray</item>
        <item name="colorPrimary">@color/LightBlue</item>
        <item name="android:colorForeground">@color/transparent_black_percent_80</item>
        <item name="android:textColorPrimary">@android:color/primary_text_light_nodisable</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="s1" parent="warden" >
        <item name="colorPrimary">@android:color/black</item>
    </style>

</resources>
